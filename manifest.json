{
	"name": "Karthic's Video Cleanup Extension",
	"action": {},
	"manifest_version": 3,
	"key": "",

	"version": "0.21",
	"author": "karthic@izonewe.com",  
	"icons": {
		"16": "img/icon16.png",
		"32": "img/icon32.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
		},
	"description": "Removes semi-transparent overlays from video's on mouseover",
	"permissions": ["webNavigation", "activeTab", "scripting", "storage", "tabs"],
	"host_permissions": [
		"*://*.netflix.com/",
		"*://*.amazon.com/",
		"*://*.max.com/",
		"*://*.mgmplus.com/"
		],

	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
		},
	"background": {
		"service_worker": "background.js", "type": "module"
		}
	}



